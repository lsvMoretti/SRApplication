<Window x:Class="UiApp.ShowMembers"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UiApp"
        mc:Ignorable="d"
        Title="Smoking Rifles - Member List" Height="800" Width="800" ResizeMode="NoResize">
    <Grid Background="#7EA172">
        <Label Name="InfoLabel" Content="" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Foreground="White" FontSize="16" Width="568" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
        <TextBox Name="SearchBox" HorizontalAlignment="Left" Margin="342,37,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="28" KeyUp="SearchBox_OnKeyUp" />
        <Button Name="SearchButton" Content="Search" HorizontalAlignment="Left" Margin="467,37,0,0" VerticalAlignment="Top" Foreground="Black" FontSize="16" Width="68" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Click="SearchButton_OnClick" />
        <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto" Margin="0,70,0,0">
            <Grid Margin="10,0,10,50" Name="MembersGrid" ShowGridLines="False">
            </Grid>
        </ScrollViewer>
    </Grid>
</Window>